package Snippet;
import java.util.ArrayList;

public class GetMinSquares {
	
	public int getMin(int n){
		int num = (int)Math.sqrt(n);
		ArrayList<Integer> que = new ArrayList<>();
		que.add(0);
		
		while(!que.isEmpty()){
			for(int i=1;i<=num;i++){
				if((i*i) == n){ return 1; }
				else{ que.add(i*i); }
			}
		}
	}
}
